            },
            {
              "type": "extension",
              "name": "message_collector",
              "addon": "message_collector",
              "extension_group": "transcriber",
              "property": {}
            }
          ],
          "connections": [
            {
              "extension": "agora_rtc",
              "cmd": [
                {
                  "name": "on_user_joined",
                  "dest": [
                    {
                      "extension": "v2v"
                    }
                  ]
                },
                {
                  "name": "on_user_left",
                  "dest": [
                    {
                      "extension": "v2v"
                    }
                  ]
                },
                {
                  "name": "on_connection_failure",
                  "dest": [
                    {
                      "extension": "v2v"
                    }
                  ]
                }
              ],
              "audio_frame": [
                {
                  "name": "pcm_frame",
                  "dest": [
                    {
                      "extension": "v2v"
                    }
                  ]
                }
              ],
              "video_frame": [
                {
                  "name": "video_frame",
                  "dest": [
                    {
                      "extension": "v2v"
                    }
                  ]
                }
              ]
            },
            {
              "extension": "v2v",
              "cmd": [
                {
                  "name": "flush",
                  "dest": [
                    {
                      "extension": "agora_rtc"
                    }
                  ]
                }
              ],
              "data": [
                {
                  "name": "text_data",
                  "dest": [
                    {
                      "extension": "message_collector"
                    }
                  ]
                }
              ],
              "audio_frame": [
                {
                  "name": "pcm_frame",
                  "dest": [
                    {
                      "extension": "agora_rtc"
                    }
                  ]
                }
              ]
            },
            {
              "extension": "message_collector",
              "data": [
                {
                  "name": "data",
                  "dest": [
                    {
                      "extension": "agora_rtc"
                    }
                  ]
                }
              ]
            }
          ]
        }
      }
    ],
    "log": {
      "level": 1
    }
  }
}